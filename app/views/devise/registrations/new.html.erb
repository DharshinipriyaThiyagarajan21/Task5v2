<form class="new_user" id="new_user" action="/users" accept-charset="UTF-8" method="post">
<div class="container" ng-app="index" ng-controller="indexController">
    <input name="utf8" type="hidden" value="âœ“">
      <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
        <div class="col-md-4"></div>
        <div class="col-md-4">
        <div class="header">
              <h1>Proj5.</h1>
            </div>
       <div class="form-group">
                <div class="right-inner-addon ">
                  <div ng-switch="signupForm.username.$valid">
                    <div ng-switch-default>
                      <span class="glyphicon glyphicon-remove-sign"></span>
                    </div>
                    <div ng-switch-when=true>
                      <span class="glyphicon glyphicon-ok-sign"></span>
                    </div>
                  </div>
                  <input type="text" ng-model="user.username" class="form-control" placeholder="Username" title="Please enter a valid Username. Alphanumeric Count should be 6-24" name="user[username]" id="user_username" required>
                </div>
              </div>

              <div class="form-group">
                <input type="text" ng-model="user.firstname" class="form-control" placeholder="First Name" name="user[firstname]" id="user_firstname" required>
              </div>

              <div class="form-group">
                <input type="text" ng-model="user.lastname" class="form-control" placeholder="Last Name" name="user[lastname]" id="user_lastname" required>
              </div>

              <div class="form-group">
                <div class="right-inner-addon ">
                  <div ng-switch="signupForm.email.$valid">
                    <div ng-switch-default>
                      <span class="glyphicon glyphicon-remove-sign"></span>
                    </div>
                    <div ng-switch-when=true>
                      <span class="glyphicon glyphicon-ok-sign"></span>
                    </div>
                  </div>
                  <input type="email" ng-model="user.email" class="form-control" placeholder="Enter email id" title="Please enter a valid email id" name="user[email]" id="user_email" required>
                </div>
              </div>

              <div class="form-group">
                <div class="right-inner-addon ">
                  <div ng-switch="signupForm.password.$valid">
                    <div ng-switch-default>
                      <span class="glyphicon glyphicon-remove-sign"></span>
                    </div>
                    <div ng-switch-when=true>
                      <span class="glyphicon glyphicon-ok-sign"></span>
                    </div>
                  </div>
                  <input type="password" ng-model="user.password" class="form-control" placeholder="Password" title="Please enter a valid password. Alphanumeric Count should be 8-12" name="user[password]" id="user_password" required>
                </div>
              </div>

              <div class="form-group">
                <div class="right-inner-addon">
                  <div ng-switch="password == passwordRepeat && passwordRepeat != null">
                    <div ng-switch-default>
                      <span class="glyphicon glyphicon-remove-sign"></span>
                    </div>
                    <div ng-switch-when=true>
                      <span class="glyphicon glyphicon-ok-sign"></span>
                    </div>
                  </div>
                  <input type="password" ng-model="passwordRepeat" class="form-control" placeholder="Confirm Password" title="Please enter the correct password" name="user[password_confirmation]" id="user_password_confirmation" required>
                </div>
              </div>

              <div class="row">
                <div class="col-md-2"></div>

                <div class="col-md-4">
                  <button type="submit" class="btn btn-default btn-signup" name="commit" data-disable-with="Sign up">Sign Up</button>
                </div>

                <div class="col-md-4">
                  <input type="button" class="btn btn-default btn-login" onclick="location.href='index.html'" value="Cancel" />
                </div>
              </div>
              </div>
              <div class="col-md-4"></div>

      </div>
      </form>









      








